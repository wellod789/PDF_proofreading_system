# ステージング環境用認証情報テンプレート
# このファイルを元にサーバーに環境変数を設定してください

# AWS設定
AWS_ACCESS_KEY_ID=your_staging_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_staging_aws_secret_access_key
AWS_DEFAULT_REGION=ap-northeast-3

# Flask設定
FLASK_ENV=staging
FLASK_DEBUG=False
SECRET_KEY=your-staging-secret-key-here-make-it-long-and-random

# 認証設定
LOGIN_ID=staging_admin
LOGIN_PASSWORD=your_staging_password

# Bedrock設定
BEDROCK_MODEL_ID=apac.anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_MODEL_ARN=

# その他の設定
MAX_PDF_PAGES=3
